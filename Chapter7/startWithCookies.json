[
  {
    "description": "这是一个会返回cookies信息的get请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "恭喜你获取cookies信息成功"
    }
  },
  {
    "description": "模拟get带cookie请求",
    "request": {
      "uri": "/get/cookie/demo",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个带cookie的get请求"
    }
  },
  {
    "description": "模拟post带cookie请求",
    "request": {
      "uri": "/post/cookie/demo",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "wangwu",
        "age": "15"
      }
    },
    "response": {
      "status": "200",
      "json": {
        "huhansan": "success",
        "age": "success"
      }
    }
  }
]